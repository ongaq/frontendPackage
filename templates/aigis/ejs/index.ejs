<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width">
	<link href="<%- root %>assets/css/highlight/okadia.css" rel="stylesheet">
	<link href="<%- root %>assets/css/theme.css" rel="stylesheet">
	<link href="<%- root %>css/style.css" rel="stylesheet">
	<style>
	p { line-height: 1.6;}

	.aigis-moduleList { margin: 20px 20px 30px;}
	.aigis-moduleList li { list-style: disc; }
	.aigis-tags__item { color: #fff !important;}
	.aigis-index > h2 { margin-top: 20px;}
	.aigis-index > ul > li { margin-top: 15px;}
	.aigis-index > ul > li:first-child { margin-top: 0;}
	.aigis-index li > ul { margin-left: 20px;}
	</style>
</head>
<body>
<div class="aigis-container">
	<%- include('sidemenu') %>

	<div class="aigis-contents">
		<div class="aigis-contents__body">
			<div class="aigis-index">
				<%- html %>
			</div>
			<% if(components.length) { %>
				<%- include('components') %>
			<% } %>
			<footer class="aigis-footer">Last update at <%- timestamp %></footer>
		</div>
	</div>
</div>
<script type="text/javascript" src="<%- root %>import.js"></script>
</body>
</html>
